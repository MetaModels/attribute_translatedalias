{
  "name": "metamodels/attribute_translatedalias",
  "description": "MetaModels attribute for translated alias",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "alias",
    "translatable"
  ],
  "type": "contao-bundle",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/attribute_translatedalias/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/attribute_translatedalias"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "^2.1@dev",
    "metamodels/core": "^2.1@dev",
    "symfony/dependency-injection": "^3.3",
    "symfony/http-kernel": "^3.3"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "^0.6",
    "phpcq/all-tasks": "^1.2",
    "metamodels/base-unit-tests": "^1.0.4"
  },
  "conflict": {
    "contao/core": "3.4.0"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeTranslatedAliasBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\AttributeTranslatedAliasBundle\\Test\\": "tests"
    }
  },
  "extra": {
    "contao-manager-plugin": "MetaModels\\AttributeTranslatedAliasBundle\\ContaoManager\\Plugin",
    "contao": {
      "transifex": {
        "project": "metamodels",
        "prefix": "attribute_translatedalias-",
        "languages_cto": "src/Resources/contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-feature/contao4": "2.1.x-dev"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.com:metamodels/core.git"
    }
  ]
}
